{
  "name": "zosai-fashion-marketplace-bot",
  "version": "1.0.0",
  "description": "ZOSAI - AI-powered Telegram bot for fashion marketplace",
  "main": "server.js",
  "scripts": {
    "start": "node --no-deprecation server.js",
    "dev": "nodemon --no-deprecation server.js",
    "install:prod": "npm install --omit=dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node src/config/migrate.js",
    "seed": "node src/config/seed.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "deploy": "npm run install:prod && railway deploy"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "redis": "^4.6.8",
    "sharp": "^0.32.6",
    "telegraf": "^4.15.6",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "license": "MIT"
}
